local delayInput, delayMethods = settingsTab.AddTextInput(
    "SetDelay",
    "Set Delay",
    "0.5",
    nil,
    function(value)
        ModernGUILibrary.createNotification("Delay set to: " .. value .. " seconds", "info")
    end
)

local autobuyRunning = false
local autobuyConnection = nil

local function buySeed(seedName)
    if seedName and seedName ~= "Default" then
        local args = { [1] = seedName }
        game:GetService("ReplicatedStorage").GameEvents.BuySeedStock:FireServer(unpack(args))
        ModernGUILibrary.createNotification("Bought: " .. seedName, "success")
    end
end

local function startAutobuy()
    local seed1 = seedMethods1.getValue()
    local seed2 = seedMethods2.getValue()
    local delayTime = tonumber(delayMethods.getValue()) or 0.5
    
    local seedQueue = {}
    if seed1 ~= "Default" then table.insert(seedQueue, seed1) end
    if seed2 ~= "Default" then table.insert(seedQueue, seed2) end
    
    if #seedQueue == 0 then
        ModernGUILibrary.createNotification("No seeds selected! Please select at least one seed.", "warning")
        return false
    end
    
    local currentIndex = 1
    local lastPurchaseTime = 0
    
    autobuyConnection = RunService.Heartbeat:Connect(function()
        if not autobuyRunning then return end
        local currentTime = tick()
        if currentTime - lastPurchaseTime >= delayTime then
            buySeed(seedQueue[currentIndex])
            currentIndex = (currentIndex % #seedQueue) + 1
            lastPurchaseTime = currentTime
        end
    end)
    return true
end

local function stopAutobuy()
    if autobuyConnection then
        autobuyConnection:Disconnect()
        autobuyConnection = nil
    end
end

local autobuyCheckbox, getAutobuyStatus = settingsTab.AddCheckbox(
    "StartAutobuy",
    "Start Autobuy (Seeds)",
    nil,
    function(checked)
        if checked then
            autobuyRunning = true
            if startAutobuy() then
                ModernGUILibrary.createNotification("Seed Autobuy started!", "success")
            else
                autobuyRunning = false
            end
        else
            autobuyRunning = false
            stopAutobuy()
            ModernGUILibrary.createNotification("Seed Autobuy stopped!", "warning")
        end
    end
)

local buyAllSeedRunning = false
local buyAllSeedConnection = nil

local function startBuyAllSeed()
    local delayTime = tonumber(delayMethods.getValue()) or 0.5
    local lastPurchaseTime = 0
    local currentSeedIndex = 1
    local allSeeds = {}
    for _, seed in ipairs(seedOptions) do
        if seed ~= "Default" then table.insert(allSeeds, seed) end
    end
    
    buyAllSeedConnection = RunService.Heartbeat:Connect(function()
        if not buyAllSeedRunning then return end
        local currentTime = tick()
        if currentTime - lastPurchaseTime >= delayTime then
            buySeed(allSeeds[currentSeedIndex])
            currentSeedIndex = (currentSeedIndex % #allSeeds) + 1
            lastPurchaseTime = currentTime
        end
    end)
    return true
end

local function stopBuyAllSeed()
    if buyAllSeedConnection then
        buyAllSeedConnection:Disconnect()
        buyAllSeedConnection = nil
    end
end

local buyAllSeedCheckbox, getBuyAllSeedStatus = settingsTab.AddCheckbox(
    "BuyAllSeed",
    "BuyAll Seed",
    nil,
    function(checked)
        if checked then
            buyAllSeedRunning = true
            if startBuyAllSeed() then
                ModernGUILibrary.createNotification("BuyAll Seed started!", "success")
            else
                buyAllSeedRunning = false
            end
        else
            buyAllSeedRunning = false
            stopBuyAllSeed()
            ModernGUILibrary.createNotification("BuyAll Seed stopped!", "warning")
        end
    end
)

